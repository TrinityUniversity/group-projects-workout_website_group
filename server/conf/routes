# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                             controllers.Application.index

GET         /home                        controllers.WorkoutPage.home
GET         /login                       controllers.WorkoutPage.login
GET         /myVideos                    controllers.WorkoutPage.myVideos
GET         /profile                     controllers.WorkoutPage.profile
GET         /search                      controllers.WorkoutPage.search
GET         /signUp                      controllers.WorkoutPage.signUp
GET         /video                       controllers.WorkoutPage.video


# Prefix must match `play.assets.urlPrefix`
GET        /assets/*file                 controllers.Assets.at(file)
GET        /versionedAssets/*file        controllers.Assets.versioned(path="/public", file: Asset)
